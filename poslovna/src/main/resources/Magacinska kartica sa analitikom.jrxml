	<queryString>
		<![CDATA[SELECT
     robna_kartica.`id` AS robna_kartica_id,
     robna_kartica.`cena` AS robna_kartica_cena,
     robna_kartica.`pocetno_stanje_kol` AS robna_kartica_pocetno_stanje_kol,
